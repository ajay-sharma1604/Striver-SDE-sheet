Question2

50 Pow(x,n)
https://leetcode.com/problems/powx-n/

//t.c- O(logn)
//s.c-O(1)
class Solution {
    public double myPow(double x, int n) {
         if (n == 0) return 1;

        double res=1;
        long val =Math.abs((long)n);
         while(val>0){
           if((val&1)!=0){
            res=res*x;
           }
           x =x*x;
           val=val>>1;
        }
    
    return n<0 ? 1/res:res;
}
}

